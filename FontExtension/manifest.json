{
    "manifest_version": 2,
  
    "name": "Sans Serify",
    "description": "This extension changes Chrome font settings to Sans Serif and back",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.jpg",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab",
     "fontSettings"
     ]
  }